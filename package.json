{
  "name": "cdqa-ui",
  "version": "1.0.8",
  "description": "A user interface for cdQA",
  "license": "Apache-2.0",
  "keywords": [
    "reading-comprehension",
    "question-answering",
    "deep-learning",
    "natural-language-processing",
    "information-retrieval",
    "bert",
    "vuejs"
  ],
  "main": "dist/cdqaUI.ssr.js",
  "module": "dist/cdqaUI.esm.js",
  "unpkg": "dist/cdqaUI.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue serve ./load-dev-dependencies.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.0.0-rc.24",
    "vue-highlight-words": "^1.2.0",
    "vue-js-toggle-button": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-loader": "^8.0.6",
    "cache-loader": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint-loader": "^2.1.2",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2"
  }
}
